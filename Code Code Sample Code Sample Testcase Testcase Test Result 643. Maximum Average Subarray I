Link:https://leetcode.com/problems/maximum-average-subarray-i/description/
class Solution {
    public double findMaxAverage(int[] nums, int k) {
        int sum=0;        
        double avgMax=0;
        for(int i=0;i<k;i++){
            sum=sum+nums[i];
        }        
        avgMax=(double)sum/k;
        for(int i=k;i<nums.length;i++){
            sum=sum+nums[i]-nums[i-k];
            avgMax=Math.max(avgMax,(double)sum/k);
        }
        return avgMax;
    }
}
