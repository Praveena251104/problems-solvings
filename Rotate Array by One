link:geeksforgeeks.org/problems/cyclically-rotate-an-array-by-one2614/1?page=1&sortBy=submissions


// // User function Template for Java

class Solution {
    public void rotate(int[] arr) {
        int n=arr.length;
        int len=1;
        for(int i=0;i<len;i++){
            int temp=arr[n-1];
            for(int j=n-2;j>=0;j--){
               arr[j+1]=arr[j];
            }
            arr[i]=temp;
        }
        
        
    }
}
